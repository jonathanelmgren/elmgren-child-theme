{
	"include": ["assets/ts/**/*"],
	"exclude": ["node_modules", "dist"],
	"compilerOptions": {
		"target": "es5" ,
		"module": "commonjs",
		"outDir": "./dist/js",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipDefaultLibCheck": true,
		"skipLibCheck": true
	}
}
